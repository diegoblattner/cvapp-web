{
  "extends": ["preact", "airbnb-base", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [0, { "varsIgnorePattern": "^h$" }],
    "import/prefer-default-export": 0,
    "comma-dangle": "error"
  }
}
